Error response from daemon: driver failed programming external connectivity on endpoint web_server_nginx-container (e1a70358bd3076e5375d1af2c8d1091c8ab8cf57f286391e16f49f37d326f667): Bind for 0.0.0.0:8081 failed: port is already allocated

# docker-compose.yml
version: "3.8"
services:
  web:
    build:
      context: ./server
      dockerfile: dockerfile
    image: web_server:latest
    container_name: web_server-container
    ports:
      - "8080:8080"
    volumes:
      - ./server:/usr/src/app
      - /usr/src/app/node_modules
    restart: always

  webphp:
    build:
      context: ./serverphp
      dockerfile: dockerfile
    image: web_server_php:latest
    container_name: web_server_php-container
    ports:
      - "8081:9000"
    volumes:
      - ./serverphp:/var/www/html
    restart: always

  portainer:
    image: portainer/portainer-ce
    container_name: portainer
    restart: always
    ports:
      - 9000:9000
      - 8000:8000
    volumes:
      - /var/run/docker.sock:/var/run/docker.sock
      - ./portainer:/data

  nginx:
    build:
      context: ./nginx
      dockerfile: Dockerfile
    image: web_server_nginx:latest
    container_name: web_server_nginx-container
    ports:
      - "80:8082"
      - "8081:9000"
    depends_on:
      - web
      - webphp
